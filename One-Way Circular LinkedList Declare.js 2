//Syntax for declaring circular linked list
//everything is simular to declaring single linked list, difference will connect the last node with the first node

//Syntax for declaring
class Node {
    constructor(data)
    {
        this.data = data;
        this.next = null;
    }
}

// Initilize and allocate memory for nodes
let first = new Node(2);
let second = new Node(3);
let Last = new Node(4)

// Connect nodes 
first.next = second;
second.next = last;
last.next = first;
//